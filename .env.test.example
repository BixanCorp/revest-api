# Test Environment Configuration
# This file contains environment variables for the test environment
# Copy this file and rename to .env.test, then fill in your actual values

# Server Configuration
PORT=8444
SPRING_PROFILES_ACTIVE=test

# Firebase Configuration for Test
# Replace with your test Firebase service account JSON
FIREBASE_CONFIG_JSON_TEST={
  "type": "service_account",
  "project_id": "your-test-firebase-project-id",
  "private_key_id": "your-test-private-key-id",
  "private_key": "-----BEGIN PRIVATE KEY-----\nYOUR_TEST_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n",
  "client_email": "firebase-adminsdk-xxxxx@your-test-project.iam.gserviceaccount.com",
  "client_id": "your-test-client-id",
  "auth_uri": "https://accounts.google.com/o/oauth2/auth",
  "token_uri": "https://oauth2.googleapis.com/token",
  "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
  "client_x509_cert_url": "https://www.googleapis.com/robot/v1/metadata/x509/firebase-adminsdk-xxxxx%40your-test-project.iam.gserviceaccount.com"
}

# Database Configuration (if needed)
# DB_HOST=test-db-host
# DB_PORT=3306
# DB_NAME=revest_test
# DB_USERNAME=test_user
# DB_PASSWORD=test_password

# CORS Configuration
CORS_ALLOWED_ORIGINS=https://test.yourdomain.com,https://staging.yourdomain.com

# Logging Level
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_BIXAN=DEBUG

# Request/Response Logging
REQUEST_LOGGING_ENABLED=false