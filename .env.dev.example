# Development Environment Configuration
# This file contains environment variables for the development environment
# Copy this file and rename to .env.dev, then fill in your actual values

# Server Configuration
PORT=8443
SPRING_PROFILES_ACTIVE=dev

# Firebase Configuration for Development
# Replace with your development Firebase service account JSON
FIREBASE_CONFIG_JSON_DEV=


# Database Configuration (if needed)
# DB_HOST=localhost
# DB_PORT=3306
# DB_NAME=revest_dev
# DB_USERNAME=dev_user
# DB_PASSWORD=dev_password

# CORS Configuration
CORS_ALLOWED_ORIGINS=https://localhost:5173,http://localhost:5173,https://localhost:3000,http://localhost:3000

# Logging Level
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_BIXAN=DEBUG

# Request/Response Logging
REQUEST_LOGGING_ENABLED=false